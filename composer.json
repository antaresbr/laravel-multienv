{
    "name": "antaresbr/laravel-multienv",
    "description": "A multienvironment (multi tenant) package for Laravel Framework",
    "homepage": "https://github.com/antaresbr/laravel-multienv",
    "keywords": [
        "php",
        "laravel",
        "multienvironment"
    ],
    "authors": [
        {
            "name": "Eduardo Guerra",
            "email": "eduardo.antares@gmail.com",
            "role": "Developer"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "antaresbr/foundation": "*",
        "laravel/framework": "^10"
    },
    "autoload": {
        "files": [
            "support/helpers.php"
        ],
        "psr-4": {
            "Antares\\Multienv\\": "src/"
        }
    },
    "require-dev": {
        "orchestra/testbench": "^8"
    },
    "autoload-dev": {
        "psr-4": {
            "Antares\\Multienv\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Antares\\Multienv\\Providers\\MultienvConsoleServiceProvider"
            ]
        }
    }
}
